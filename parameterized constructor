public class Flower {
    String name;
    String color;
    int size;
    
    public Flower(String name, String color, int size){
        this.name = name;
        this.color = color;
        this.size = size;
    }
   
    public static void main(String[] args) {
        Flower f = new Flower("Rose","Red",10);
    
        System.out.println(f.name+" "+ f.color+" "+f.size);
    }
    
}
